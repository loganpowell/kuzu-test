{
  "name": "@kuzu-auth/client",
  "version": "0.1.0",
  "description": "Browser SDK for KuzuDB WASM authorization - <1ms client-side permission checks",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --tsconfig tsconfig.build.json --external kuzu-wasm --external idb",
    "dev": "vite",
    "preview": "vite preview",
    "benchmark": "vite --open /benchmark.html",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "authorization",
    "kuzu",
    "wasm",
    "permissions",
    "client-side",
    "browser",
    "offline-first"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "idb": "^8.0.0",
    "kuzu-wasm": "^0.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "@vitest/ui": "^4.0.16",
    "fake-indexeddb": "^6.2.5",
    "happy-dom": "^20.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.16"
  }
}
